FoamFile
{
    version     2.0;
    format      binary;
    arch        "LSB;label=32;scalar=64";
    class       volScalarField;
    location    "0";
    object      p_rgh;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    sidewalls1
    {
       type            empty;
        gradient        uniform 0;
        value           uniform 0;
    }
    sidewalls2
    {
       type            empty;
        gradient        uniform 0;
        value           uniform 0;
    }
/* inlet
    {
        type gabcPressureRobinV;                                                                                                                                                                                                                     value uniform 0;                                                                                                                                                                                                                             fixedCoeffs uniform 0;                                                                                                                                                                                                                       gradCoeffs  uniform 1;                                                                                                                                                                                                                       source      uniform 0; 
    }
    outlet
    {
        type gabcPressureRobinV;                                                                                                                                                                                                                     value uniform 0;                                                                                                                                                                                                                             fixedCoeffs uniform 0;                                                                                                                                                                                                                       gradCoeffs  uniform 1;                                                                                                                                                                                                                       source      uniform 0;
    }*/

  inlet { type fixedFluxPressure; }
  outlet{ type fixedFluxPressure; }
    atmosphere
    {
        type            totalPressure;
        rho             rho;
        psi             none;
        gamma           1;
        p0              uniform 0;
        value           uniform 0;
    }
    floater
    {
        type            fixedFluxExtrapolatedPressure;
        gradient        uniform 0;
        value           uniform 0;
    }
    bottom
    {
       type             fixedFluxExtrapolatedPressure;
       gradient         uniform 0;
       value            uniform 0;
    }
}


